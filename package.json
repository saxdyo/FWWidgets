{
  "name": "tmdb-backdrop-scraper",
  "version": "1.0.0",
  "description": "TMDB电影背景图爬取工具",
  "main": "tmdb-scraper.js",
  "scripts": {
    "start": "node tmdb-scraper.js",
    "generate:preprocessed": "node create-preprocessed-data.js",
    "deploy:preprocessed": "./deploy-preprocessed-data.sh",
    "test:preprocessed": "curl -s 'https://raw.githubusercontent.com/saxdyo/FWWidgets/main/data/TMDB_Trending.json' | jq '.today_global | length'"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.3.0"
  },
  "keywords": [
    "tmdb",
    "movie",
    "backdrop"
  ],
  "author": "saxdyo",
  "license": "MIT"
}
